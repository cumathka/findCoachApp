<base-card>\n        <h2>Find Your Coach</h2>\n        <span class=\"filter-option\">\n          <input\n            type=\"checkbox\"\n            name=\"frontend\"\n            id=\"frontend\"\n            checked\n            @change=\"setFilter\"\n          />\n          <label for=\"frontend\">Frontend</label>\n        </span>\n        <span class=\"filter-option\">\n          <input\n            type=\"checkbox\"\n            name=\"backend\"\n            id=\"backend\"\n            checked\n            @change=\"setFilter\"\n          />\n          <label for=\"backend\">Backend</label>\n        </span>\n        <span class=\"filter-option\">\n          <input\n            type=\"checkbox\"\n            name=\"career\"\n            id=\"career\"\n            checked\n            @change=\"setFilter\"\n          />\n          <label for=\"career\">Career</label>\n        </span>\n      </base-card>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    emits: ['change-filter'],\n    data() {\n      return {\n        filters: {\n          frontend: true,\n          backend: true,\n          career: true,\n        },\n      };\n    },\n    methods: {\n      setFilter(event) {\n        const inputId = event.target.id;\n        const isActive = event.target.checked;\n        this.filters[inputId] = isActive;\n        this.$emit('change-filter', { ...this.filters });\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  h2 {\n    margin: 0.5rem 0;\n  }\n  \n  .filter-option {\n    margin-right: 1rem;\n  }\n  \n  .filter-option label,\n  .filter-option input {\n    vertical-align: middle;\n  }\n  \n  .filter-option label {\n    margin-left: 0.25rem;\n  }\n  \n  .filter-option.active label {\n    font-weight: bold;\n  }\n  </style>\n  "]}]}